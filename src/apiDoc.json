{
  "swagger": "2.0",
  "info": {
    "title": "The open weather api adapter.",
    "version": "1.0.0"
  },
  "definitions": {
    "DailyWeather": {
      "type": "object",
      "properties": {
        "date": {
          "type": "string",
          "format": "date",
          "example": "2020-10-15"
        },
        "temperature": {
          "type": "number",
          "format": "float"
        },
        "weatherCondition": {
          "type": "string",
          "enum": [
            "sunny",
            "cloudy",
            "rainy",
            "snowy",
            "windy"
          ]
        }
      }
    }
  },
  "paths": {}
}
